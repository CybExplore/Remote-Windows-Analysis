{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WorkerSettings": {
    "ApiBaseUrl": "http://localhost:8002/",
    "LogFilePath": "logs.json",
    "ProcessLogFilePath": "process_logs.json",
    "NetworkLogFilePath": "network_logs.json",
    "FileLogFilePath": "file_logs.json",
    "MonitorPath": "C:\\Windows\\System32",
    "BatchSize": 100,
    "PollIntervalSeconds": 5,
    "RetryDelaySeconds": 60,
    "FrontendUrl": "http://localhost:3000/",
    "LogNames": ["System", "Application", "Security"]
  }
}


